<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>vlocity_ps__WaitlistRemovedReasonNotNull</fullName>
    <active>true</active>
    <errorConditionFormula>IF( vlocity_ps__IsRemoved__c == true &amp;&amp; ISBLANK( vlocity_ps__ReasonForRemoval__c ) , true, false)</errorConditionFormula>
    <errorDisplayField>vlocity_ps__ReasonForRemoval__c</errorDisplayField>
    <errorMessage>Please select reason/s for removal</errorMessage>
</ValidationRule>
